{
  "name": "tbmelabs-authorization-server",
  "description": "The webapplication for the TBME Labs authorization server.",
  "version": "1.0.0-SNAPSHOT",
  "author": "TBME Labs",
  "repository": "https://github.com/tbmelabs/tbmelabs-authorization-server.git",
  "bugs": {
    "url": "https://github.com/tbmelabs/tbmelabs-authorization-server/issues"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "yarn run build:browser:prod && yarn run build:server:prod && yarn run webpack:server",
    "build:dev": "yarn run build:browser && yarn run build:server && yarn run webpack:server",
    "build:browser": "ng build",
    "build:browser:watch": "ng build --watch",
    "build:browser:prod": "yarn run build:browser --prod --deploy-url=/public/ --delete-output-path --progress",
    "build:server": "ng run tbme-labs-authorization-server:server",
    "build:server:prod": "ng run tbme-labs-authorization-server:server:production --delete-output-path --progress",
    "webpack:server": "webpack --config webpack.server.config.js --progress",
    "test": "ng test",
    "test:watch": "yarn run test --watch",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "6.1.1",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/platform-server": "^6.1.10",
    "@angular/router": "^6.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.1.0",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "npm": "^6.4.1",
    "popper.js": "^1.14.4",
    "rxjs": "~6.2.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "~6.2.5",
    "@angular/compiler-cli": "6.1.1",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^3.3.17",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  }
}
